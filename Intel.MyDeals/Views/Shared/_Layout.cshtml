<!DOCTYPE html>
<html ng-app="app" ng-strict-di>

<head>
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - MyDeals</title>

    @Styles.Render("~/Content/css")
    @Styles.Render("~/MyDeals/css")

    @Scripts.Render("~/bundles/scripts")
    @Scripts.Render("~/MyDeals/scripts")
</head>

<body>
    <admin-banner></admin-banner>
    @Html.Partial("_Header")
    <div id="modals"></div>
    <div id="bodyContent" style="position: absolute; top: 78px; bottom: 0; left: 0; right: 0; overflow: auto;">
        @Html.Partial("_Loading")
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/angular")
    <!-- inject:js -->
    <!-- Bootstrapping -->

    @Scripts.Render("~/bundles/angularReusable")
    @Scripts.Render("~/bundles/angularModules")


</body>

</html>

