{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "types": ["node", "jquery"],
    "typeRoots": [
      "../node_modules/@types"
    ],
    "forceConsistentCasingInFileNames": false
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "temp"
  ]
}